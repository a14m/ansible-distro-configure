// Firefox user.js - User preferences
// This file is automatically generated by Ansible
// Manual changes will be overwritten

{% for pref, value in firefox_user_prefs.items() %}
{% if value is string %}
user_pref("{{ pref }}", "{{ value }}");
{% elif value is boolean %}
user_pref("{{ pref }}", {{ value | lower }});
{% else %}
user_pref("{{ pref }}", {{ value }});
{% endif %}
{% endfor %}
