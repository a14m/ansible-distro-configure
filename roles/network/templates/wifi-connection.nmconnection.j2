[connection]
id={{ network_wifi_ssid }}
uuid={{ network_wifi_ssid | to_uuid }}
type=wifi
autoconnect=true

[wifi]
mode=infrastructure
ssid={{ network_wifi_ssid }}

[wifi-security]
auth-alg=open
key-mgmt=wpa-psk
psk={{ network_wifi_pass }}

[ipv4]
method=auto

[ipv6]
addr-gen-mode=default
method=auto

[proxy]
